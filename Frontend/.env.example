# Frontend Environment Variables

# Backend API (set to "true" to use backend instead of direct OpenRouter calls)
VITE_USE_BACKEND=false
VITE_BACKEND_URL=http://localhost:3001

# OpenRouter API (only needed if VITE_USE_BACKEND is false)
VITE_OPENROUTER_API_KEY=your_openrouter_api_key_here
VITE_OPENROUTER_MODEL=qwen/qwen3-vl-32b-instruct:free

# Supabase - imageQwen Project
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here
